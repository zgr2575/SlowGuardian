{
  "name": "slowguardian",
  "version": "9.0.0",
  "main": "index.js",
  "description": "A powerful frontend powered by Ultraviolet with modern UI and plugin system",
  "test-sw-server": "node scripts/test-server.js",
  "test-sw-headless": "node scripts/test-sw-puppeteer.js",
  "keywords": [
    "proxy",
    "web-proxy",
    "bypass",
    "unblock",
    "ultraviolet"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "npm run build:version && node index.js",
    "dev": "npm run build:version && node --watch index.js",
    "format": "npx prettier . --write",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "node --test",
    "build": "npm run build:version && npm run format",
    "build:production": "NODE_ENV=production npm run build:version && npm run format && npm run audit-fix",
    "build:version": "node scripts/build-version.js",
    "audit-fix": "npm audit fix",
    "upd": "git pull --force --allow-unrelated-histories"
  },
  "author": "zgr2575",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/zgr2575/SlowGuardian.git"
  },
  "dependencies": {
    "@mercuryworkshop/epoxy-transport": "^2.1.28",
    "@mercuryworkshop/libcurl-transport": "^1.5.0",
    "@mercuryworkshop/scramjet": "^1.0.2",
    "@nebula-services/dynamic": "^0.7.2-patch.2",
    "@replit/database": "^3.0.1",
    "@rubynetwork/rammerhead": "^1.3.6",
    "@titaniumnetwork-dev/ultraviolet": "^3.2.10",
    "@tomphttp/bare-server-node": "2.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.19.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "3.2.5"
  }
}
