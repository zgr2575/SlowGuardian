{
  "name": "slowguardian",
  "version": "9.0.0",
  "type": "module",
  "description": "A powerful frontend powered by Ultraviolet with modern UI and plugin system",
  "keywords": [
    "proxy",
    "web-proxy",
    "bypass",
    "unblock",
    "ultraviolet"
  ],
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "format": "npx prettier . --write",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "node --test",
    "build": "npm run format && npm run lint",
    "audit-fix": "npm audit fix",
    "upd": "git pull --force --allow-unrelated-histories"
  },
  "author": "zgr2575",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/zgr2575/SlowGuardian.git"
  },
  "dependencies": {
    "@replit/database": "^3.0.1",
    "@tomphttp/bare-server-node": "2.0.3",
    "cors": "^2.8.5",
    "express": "4.18.3",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2"
  },
  "resolutions": {
    "gopd": "1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "3.2.5"
  }
}
